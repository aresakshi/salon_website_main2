<!DOCTYPE html>
<html lang="en">
<head>
    <title>Client List</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
<!-- <%- include("navbar.ejs") %> -->
 <%- include('./navbar.ejs') %>
<div class="admin-main">
    <div class="container-fluid bg-white p-4 mt-4 rounded shadow-sm">
        <h3>Client Contact List</h3>
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Sr No</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Message</th>
                </tr>
            </thead>
            <tbody>
                <% client.forEach(function(val, i){ %>
                <tr>
                    <td><%= i + 1 %></td>
                    <td><%= val.client_name %></td>
                    <td><%= val.client_email %></td>
                    <td><%= val.client_phone %></td>
                    <td><%= val.client_message %></td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</div>
<%- include("footer") %>
</body>
</html>